{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended",
        "helpers:pinGitHubActionDigests",
        "customManagers:githubActionsVersions"
    ],
    "packageRules": [
        {
            "matchManagers": [
                "gomod"
            ],
            "matchDepTypes": [
                "indirect"
            ],
            "enabled": true
        },
        {
            "matchManagers": [
                "github-actions"
            ],
            "groupName": "github actions",
            "groupSlug": "github-actions"
        }
    ]
}
